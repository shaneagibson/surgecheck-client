<?xml version='1.0' encoding='utf-8'?>
<widget id="uk.co.epsilontechnologies.ClientApp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="PushAppsPlugin">
        <param name="android-package" value="com.pushapps.phonegap.PushAppsPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Toast">
        <param name="android-package" value="nl.xservices.plugins.Toast" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LaunchMyApp">
        <param name="android-package" value="nl.xservices.plugins.LaunchMyApp" />
    </feature>
    <name>Epsilon</name>
    <description>Epsilon Client App</description>
    <author email="sgibson@epsilontechnologies.co.uk" href="http://www.epsilontechnologies.co.uk" />
    <content src="index.html" />
    <access origin="*" />
</widget>
